{
  "name": "davehudson.io",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/parser": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "babel": "^6.23.0",
    "babel-loader": "^9.1.3",
    "esbuild": "^0.21.5",
    "esbuild-plugin-tsc": "^0.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/src/**/*.test.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/src/**/test.ts",
      "!**/src/**/*.test.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "dependencies": {
    "express": "^4.19.2",
    "puppeteer": "^22.13.0"
  }
