    "jsdom": "^24.0.0",
    "rimraf": "^5.0.5"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/**/*.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
